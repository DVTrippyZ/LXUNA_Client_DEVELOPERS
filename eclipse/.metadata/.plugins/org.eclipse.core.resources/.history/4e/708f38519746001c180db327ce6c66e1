package intent.ui;

public class Rainbow {

	
	
	public static int render(String fade) {
		
		int min = 100000;  
		int max = 900000;  
		String y = "0x" + fade;
	
		
		
		int random = (int)Math.random()*(max-min+1)+min;
		
		
		return reTypeInt(y + reTypeString(random));
		
		
	}
	
	
	
	
	private static String reTypeString(int i) {
		return Integer.toString(i);
		}
	
	private static int reTypeInt(String s) {
		return Integer.parseInt(s);
		}
	
	
}
